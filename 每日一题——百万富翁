import java.util.HashMap;
import java.util.Map;
import java.util.Scanner;

public class 百万富翁问题 {
    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int wan=0;
        int fen=0;
        int sum=1;
        for(int i=1;i<=30;i++){
            wan+=10;
            fen+=sum;
            sum*=2;
        }
        System.out.println(wan+" "+fen);
    }
}
