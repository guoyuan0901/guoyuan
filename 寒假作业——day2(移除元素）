package day2;

public class 移除元素 {
    //去掉数组中val的元素，并返回数组删除后的长度
    class Solution {
        public int removeElement(int[] nums, int val) {
            int count=0;
            for(int i=0;i<nums.length;i++){
                if(nums[i]!=val){
                    nums[count]=nums[i];
                    count++;
                }
            }
            return count;
        }

    }
}
